m4_define([[[RELEASE_NV]]],[[[$RELEASE_NV]]])m4_dnl
m4_define([[[CTU_XSL1_NAMESPACE]]],[[[$CTU_XSL1_NAMESPACE]]])m4_dnl
m4_define([[[CTU_XSL2_NAMESPACE]]],[[[$CTU_XSL2_NAMESPACE]]])m4_dnl
EOF
